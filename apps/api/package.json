{
  "name": "@fastify-ts-template/api",
  "version": "1.0.0",
  "description": "API service for Fastify TypeScript template",
  "scripts": {
    "dev": "NODE_ENV=development tsx --watch src/server.ts | pino-pretty",
    "build": "esbuild 'src/**/*.ts' --outdir=dist --platform=node --format=cjs --minify && tsc-alias -p tsconfig.json",
    "start": "NODE_ENV=production node dist/server.js",
    "prisma:generate": "prisma generate",
    "typecheck": "tsc",
    "test": "vitest run",
    "test:dev": "vitest dev",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@fastify/autoload": "^6.3.1",
    "@fastify/cors": "^11.1.0",
    "@fastify/env": "^5.0.3",
    "@fastify/jwt": "^10.0.0",
    "@fastify/redis": "^7.1.0",
    "@fastify/swagger": "^9.5.2",
    "@fastify/swagger-ui": "^5.2.3",
    "@fastify/type-provider-typebox": "^6.1.0",
    "@prisma/client": "^6.18.0",
    "close-with-grace": "^2.3.0",
    "fastify": "^5.6.1",
    "typebox": "^1.0.43",
    "typescript": "~5.9.3"
  },
  "devDependencies": {
    "@types/node": "^22.18.12",
    "@vitest/coverage-v8": "^4.0.2",
    "esbuild": "^0.25.11",
    "pino-pretty": "^13.1.2",
    "prisma": "^6.18.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.2"
  }
}
